{
	"content_scripts": [
		{
			"js": [
				"lib/lodash.min.js",
				"main.js"
			],
			"matches": [
				"*://pokevision.com/*"
			]
		}
	],
	"icons": {
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"permissions": [
		"storage"
	],
	"web_accessible_resources": [
		"lib/lodash.min.js",
		"resources/dex.js",
		"resources/names.js",
		"injected.js",
		"injected.css"
	],
	"description": "Better Pokémon species filter for PokéVision",
	"manifest_version": 2,
	"name": "PokéVision Filter",
	"version": "1.3.2",
	"applications": {
		"gecko": {
			"id": "pokevision-filter@willnow.xyz",
			"strict_min_version": "42.0",
			"strict_max_version": "50.*"
		}
	}
}
