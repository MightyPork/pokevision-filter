{
	"content_scripts": [
		{
			"js": [
				"lib/lodash.min.js",
				"main.js"
			],
			"matches": [
				"*://pokevision.com/*"
			]
		}
	],
	"icons": {
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"permissions": [
		"storage"
	],
	"web_accessible_resources": [
		"injected.js",
		"lib/lodash.min.js",
		"injected.css"
	],
	"description": "Pokémon species filter for PokéVision.",
	"manifest_version": 2,
	"name": "PokéVision Filter",
	"version": "1.2.0"
}
